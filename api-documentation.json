{
  "openapi": "3.0.0",
  "info": {
    "title": "Drone Routes Service API",
    "version": "1.0.0",
    "description": "MVP service for drone multispectral photo routes",
    "contact": {
      "name": "Drone Routes Team"
    }
  },
  "servers": [
    {
      "url": "http://localhost:3000",
      "description": "Development server"
    }
  ],
  "paths": {
