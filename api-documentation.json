    "/api/v1/routes/:id/photos": {
      "post": {
        "tags": [
          "routes"
        ],
        "summary": "Create Routes",
        "description": "POST operation on routes",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "id identifier"
          }
        ],
        "responses": {
          "201": {
            "description": "Created",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/routesResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
